{
  "expo": {
    "name": "Bio/vizion",
    "slug": "PunchInOutMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/biovizion.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Permite acceder a tus fotos para seleccionar una imagen de perfil",
        "NSPhotoLibraryAddUsageDescription": "Permite guardar imágenes en tu galería"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.edwinnunez.PunchInOutMobile",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicación necesita acceso a tus fotos para que puedas seleccionar una imagen de perfil."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "469adcd0-4e35-4125-80a8-68065263f6ff"
      }
    }
  }
}